package desafios;

public class DesafioFor {
	public static void main(String[] args) {
		int controle = 1;
		int controle2 = 5;
		String valor = "#";
		
		for(int i = controle; i <= controle2; i++) {
			System.out.println(valor);
			valor += "#";
		}
		
		for(String v = "#"; !v.equals("######"); v += "#") {
			System.out.println(v);
		}
	}
}
