package desafios;

public class Produto {
	String nome;
	double preco;
	static double desconto = 0.25;
	
	Produto(String nome, double preco){
		this.nome = nome;
		this.preco = preco;
	}
	
	double descontoPadrao() {
		double precoTotal = preco * (1 - desconto);
		String nomeProduto = nome;
		return nome + precoTotal;
	}
	
//	static double descontoMaes() {
