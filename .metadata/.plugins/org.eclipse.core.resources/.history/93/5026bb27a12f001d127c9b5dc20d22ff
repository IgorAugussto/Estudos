package excecao;

public class Basico {
	
	public static void main(String[] args) {
		
		Aluno aluno = null;
		
		try {
			imprimirNomdeAluno(aluno);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		System.out.println(7 / 0);
		
		
		
	}
	
	public static void imprimirNomdeAluno(Aluno aluno) {
		System.out.println(aluno.nome);
	}
}
