package desafioFinal;

public class Pessoa {
	
	String nome;
	static double peso = 80.000;
	
	Comida comida = new Comida();
	
	Pessoa(String nome){
		this.nome = nome;
	}
	
	double comerPratoPrincipal() {
		double comidaNoCorpo = comida.pesoComida + peso;
		return comidaNoCorpo;
	}
	
	double comerSobremesa() {
		double sobremesaNoCorpo = comida.pesoComida + peso;
		return sobremesaNoCorpo;
	}
	
}
