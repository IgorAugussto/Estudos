		System.out.printf("Produto: %s\nPreço: %.2f", p1.nome, p1.descontoPadrao()); 
		System.out.println("DESCONTO DE DIA DAS MÃES!!");
		System.out.println("DESCONTO DE NATAL!!");
		
		
	}
}
