package desafios;

public class DataTeste {
	public static void main(String[] args) {
		
		Data data1 = new Data();
		data1.dia = "19";
		data1.mes = "12";
		data1.ano = 1982;
		
		System.out.printf("%s/%s/%d", data1.dia, data1.mes, data1.ano);
		
		Data data2 = new Data();
		data2.dia = "24";
		data2.mes = "11";
		data2.ano = 2013;
		
		System.out.printf("\n%s/%s/%d", data2.dia, data2.mes, data2.ano);
		
		
	}
}
