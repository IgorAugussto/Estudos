package desafios;

import java.util.Scanner;

public class desafioScanner {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		String valor1 = System.out.println("Digite o seu primeiro salário: ");
		entrada.nextLine();
		String valor2 =  System.out.println("\nDigite o seu segundo salário: ");
		entrada.nextLine();
		String valor3 = System.out.println("\nDigite o seu terceito salário: ");
		entrada.nextLine();
		
		double numero1 = Double.parseDouble(valor1);
		double numero2 = Double.parseDouble(valor2);
		double numero3 = Double.parseDouble(valor3);
		
		
		
		
	}
}
